{
    "name": "test_EMDiffuse-n",
    "norm": true,
    "percent": false,
    "gpu_ids": [
        0
    ],
    "seed": -1,
    "finetune_norm": false,
    "task": "denoise",
    "path": {
        "base_dir": "experiments",
        "code": "code",
        "tb_logger": "tb_logger",
        "results": "results",
        "checkpoint": "checkpoint",
        "resume_state": "experiments/EMDiffuse-n/2720"
    },
    "datasets": {
        "train": {
            "which_dataset": {
                "name": [
                    "data.dataset",
                    "EMDiffusenDataset"
                ],
                "args": {
                    "data_root": "/data/cxlu/macos_backup/EMDiffuse_dataset/denoise/train_wf",
                    "data_len": -1,
                    "norm": true,
                    "percent": false
                }
            },
            "dataloader": {
                "validation_split": 0.1,
                "args": {
                    "batch_size": 3,
                    "num_workers": 4,
                    "shuffle": true,
                    "pin_memory": true,
                    "drop_last": true
                },
                "val_args": {
                    "batch_size": 10,
                    "num_workers": 4,
                    "shuffle": false,
                    "pin_memory": true,
                    "drop_last": false
                }
            }
        },
        "test": {
            "which_dataset": {
                "name": "EMDiffusenDataset",
                "args": {
                    "data_root": "./demo/denoise_crop_patches",
                    "norm": true,
                    "percent": false,
                    "phase": "val"
                }
            },
            "dataloader": {
                "args": {
                    "batch_size": 8,
                    "num_workers": 4,
                    "pin_memory": true
                }
            }
        }
    },
    "model": {
        "which_model": {
            "name": [
                "models.EMDiffuse_model",
                "DiReP"
            ],
            "args": {
                "sample_num": 8,
                "task": "denoise",
                "ema_scheduler": {
                    "ema_start": 1,
                    "ema_iter": 1,
                    "ema_decay": 0.9999
                },
                "optimizers": [
                    {
                        "lr": 5e-05,
                        "weight_decay": 0
                    }
                ]
            }
        },
        "which_networks": [
            {
                "name": [
                    "models.EMDiffuse_network",
                    "Network"
                ],
                "args": {
                    "init_type": "kaiming",
                    "module_name": "guided_diffusion",
                    "norm": true,
                    "unet": {
                        "in_channel": 2,
                        "out_channel": 1,
                        "inner_channel": 32,
                        "channel_mults": [
                            1,
                            2,
                            4,
                            8
                        ],
                        "attn_res": [
                            16
                        ],
                        "num_head_channels": 32,
                        "res_blocks": 2,
                        "dropout": 0.2,
                        "image_size": 256
                    },
                    "beta_schedule": {
                        "train": {
                            "schedule": "linear",
                            "n_timestep": 2000,
                            "linear_start": 1e-06,
                            "linear_end": 0.01
                        },
                        "test": {
                            "schedule": "linear",
                            "n_timestep": 1000,
                            "linear_start": 0.0001,
                            "linear_end": 0.09
                        }
                    }
                }
            }
        ],
        "which_losses": [
            "mse_loss"
        ],
        "which_metrics": [
            "mae"
        ]
    },
    "train": {
        "n_epoch": 100000000.0,
        "n_iter": 100000000.0,
        "val_epoch": 20,
        "save_checkpoint_epoch": 20,
        "log_iter": 10000.0,
        "tensorboard": true
    },
    "debug": {
        "val_epoch": 1,
        "save_checkpoint_epoch": 1,
        "log_iter": 10,
        "debug_split": 50
    },
    "phase": "test",
    "distributed": false
}